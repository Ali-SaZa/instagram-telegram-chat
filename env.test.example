# Test Environment Configuration for Phase 4 Services

# Environment
ENVIRONMENT=testing
DEBUG=true

# Database (MongoDB)
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=instagram_telegram_chat_test
MONGODB_MAX_POOL_SIZE=5
MONGODB_MIN_POOL_SIZE=1

# Redis (for message queue)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DATABASE=1
REDIS_MAX_CONNECTIONS=5

# Instagram (test credentials)
INSTAGRAM_USERNAME=test_user
INSTAGRAM_PASSWORD=test_password
INSTAGRAM_SESSION_FILE=test_instagram_session.json
INSTAGRAM_SYNC_INTERVAL=60
INSTAGRAM_MAX_RETRIES=2
INSTAGRAM_RETRY_DELAY=30

# Telegram (test bot token)
TELEGRAM_BOT_TOKEN=test_bot_token_12345
TELEGRAM_WEBHOOK_PORT=8443
TELEGRAM_POLLING_TIMEOUT=10
TELEGRAM_MAX_CONNECTIONS=10

# Logging
LOG_LEVEL=DEBUG
LOG_FILE=logs/test.log

# Security
ENCRYPTION_KEY=test_encryption_key_12345
ENABLE_ENCRYPTION=false
API_RATE_LIMIT=1000
SESSION_TIMEOUT=1800

# Feature flags
ENABLE_WEBHOOKS=true
ENABLE_SYNC_SERVICE=true
ENABLE_TELEGRAM_BOT=true 